  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongodb": "^3.1.3",
    "mongoose": "^5.2.7",
    "next": "^6.1.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-flip-move": "^3.0.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-thunk": "^1.0.0",
    "reactstrap": "^6.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "2.2.0",
    "validator": "^10.5.0"
  },
  "scripts": {
    "server": "babel-node server/server.js --presets es2015,stage-2,react",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev-next": "next",
    "dev-build": "next build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test-client": "react-scripts test --env=jsdom",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js --timeout 20000",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.6.1",
    "dotenv": "^6.0.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "expect": "^23.5.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "react-test-renderer": "^16.4.2",
    "supertest": "^3.1.0"
  }
}
